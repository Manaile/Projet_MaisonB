RewriteEngine On
RewriteBase /

RewriteRule ^index\.example\.php$ - [R=404,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^(.*)$ index.php?url=$1 [QSA,PT,L]
